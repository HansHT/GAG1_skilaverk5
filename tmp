
SELECT 'Boats: BL --> BNo' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT X.BL
    FROM Boats X
    GROUP BY X.BL
    HAVING COUNT(DISTINCT X.BNo) > 1
);


SELECT 'Boats: BL --> Z' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT X.BL
    FROM Boats X
    GROUP BY X.BL
    HAVING COUNT(DISTINCT X.Z) > 1
);


SELECT 'Boats: BL --> T' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT X.BL
    FROM Boats X
    GROUP BY X.BL
    HAVING COUNT(DISTINCT X.T) > 1
);


